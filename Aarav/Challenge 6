PA = 0
class Player:
  def __init__(playerName, randomNumber, numberOfAttempts):
    playerName = name
    randomNumber = number
    numberOfAttempts = numberOfAttempts

  def incrementNumberOfAttempts(self):
    PA = PA + 1

  def isGuessNumberValid(self, guessNumber):
    while True:
      guess = int(input("Guess a number between 1 and 100:"))
      if guess == number: 
        print("You Won!!")
        break
      elif guess < number:
        print("You guessed low.") 
      else:
        print("You guessed high.")

import random 
  
number = random.randint(1, 101)

Player1 = Player()
Player1.name = input("Enter player 1 name: ")
Player1.number = number
Player1.numberOfAttempts = PA

